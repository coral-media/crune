{
    "name": "coral-media/crune",
    "description": "(C)onsole (Run)time (E)nviroment. Ideal for running cron jobs and low profile/level tasks.",
    "keywords": ["cli", "console", "command", "cronjobs", "symfony"],
    "type": "project",
    "license": "MIT",
    "version": "v0.1.4",
    "authors": [
        {
            "name": "Rafael Ernesto Espinosa Santiesteban",
            "email": "rernesto.espinosa@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "symfony/console": "^6.2",
        "symfony/dependency-injection": "^6.2",
        "symfony/yaml": "^6.2",
        "symfony/config": "^6.2",
        "symfony/dotenv": "^6.2",
        "symfony/process": "^6.2"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^6.2",
        "symfony/var-dumper": "^6.2",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Test\\": "tests/"
        }
    }
}
